void dispImage(PROGMEM uint32_t img [2048]){
  int tot = 0;
  for(int i = 0; i < 32; i++){
    for(int j = 0; j < 64; j++){
      uint32_t dspPixel = pgm_read_dword_far(img + tot);
      show_pixel(j, i, dspPixel);
//      Serial.println(dspPixel, HEX);
      tot ++;
    }
  }
}

uint8_t hex_to_red(uint32_t hex){
  return ((hex & 0xff0000) >> 16);
}

uint8_t hex_to_green(uint32_t hex){
  return ((hex & 0x00ff00) >> 8);
}

uint8_t hex_to_blue(uint32_t hex){
  return hex & 0x0000ff;
}

void show_pixel(byte x, byte y, uint32_t hexValue){
  matrix.drawPixel(x, y, matrix.Color888(hex_to_red(hexValue), hex_to_green(hexValue), hex_to_blue(hexValue)));
}


