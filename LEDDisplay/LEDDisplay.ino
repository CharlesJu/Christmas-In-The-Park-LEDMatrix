  matrix.begin();
  Serial.println("setup");

  matrix.fillScreen(10);
  delay(1000);
  uint32_t high = 0xffffff;

  int tot = 0;
  for(int i = 0; i < 32; i++){
    for(int j = 0; j < 64; j++){
      uint32_t dspPixel = pgm_read_dword_far(horse + tot);
      show_pixel(j, i, dspPixel);
      
