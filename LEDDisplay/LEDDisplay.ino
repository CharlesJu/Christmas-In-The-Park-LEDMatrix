      Serial.print(tot);
      Serial.print("  R:");
      Serial.print(hex_to_red(dspPixel));
      Serial.print("  G:");
      Serial.print(hex_to_green(dspPixel));
      Serial.print("  B:");
      Serial.print(hex_to_blue(dspPixel));
      Serial.print("  Hex:");
      Serial.println(dspPixel, HEX);
      tot ++;
    }
  }
  
